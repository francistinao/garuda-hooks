    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.3",
    "rollup-plugin-dts": "^6.3.0",
    "size-limit": "^12.0.0",
